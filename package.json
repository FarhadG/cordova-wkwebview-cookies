    "start:ios": "cordova start ios --buildFlag='-UseModernBuildSystem=0'"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-ionic-webview": "^4.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.16.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "ios"
    ]
  },
  "devDependencies": {}
